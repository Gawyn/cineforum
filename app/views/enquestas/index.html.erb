<h1>Listing enquestas</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Body</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @enquestas.each do |enquesta| %>
  <tr>
    <td><%= enquesta.title %></td>
    <td><%= enquesta.body %></td>
    <td><%= link_to 'Show', enquesta %></td>
    <td><%= link_to 'Edit', edit_enquesta_path(enquesta) %></td>
    <td><%= link_to 'Destroy', enquesta, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Enquesta', new_enquesta_path %>
